header(itemscope itemtype="http://schema.org/WPHeader").main-header
  .ed-grid.cols-s-5.cols-lg-10
    .span-s-2.span-lg-2.cross-center(itemscope itemtype="http://schema.org/Organization")
      a(href="/" itemprop="url")
        img(alt="Logotipo de EDgrid" itemprop="logo" src="/assets/img/EDgrid-logo.svg").main-logo
    .span-s-3.span-lg-8.main-menu-container.cross-center.main-end
      nav(itemscope itemtype="http://schema.org/SiteNavigationElement" role="navigation").main-menu#main-menu
        include main-menu
      .main-menu-toggle.to-l#main-menu-toggle

